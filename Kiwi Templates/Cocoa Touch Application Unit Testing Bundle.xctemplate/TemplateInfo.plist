<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Identifier</key>
    <string>com.modocache.templates.project.cocoaTouchApplicationUnitTestBundle</string>

    <key>Kind</key>
    <string>Xcode.Xcode3.ProjectTemplateUnitKind</string>

    <key>Ancestors</key>
    <array>
        <string>com.modocache.templates.project.objectiveCUnitTestBundle</string>
        <string>com.apple.dt.unit.iPhoneBase</string>
    </array>

    <key>Options</key>
    <array>
        <dict>
            <key>Identifier</key>
            <string>coreData</string>
            <key>Units</key>
            <dict>
                <key>true</key>
                <dict>
                    <key>Targets</key>
                    <array>
                        <dict>
                            <key>Frameworks</key>
                            <array>
                                <string>CoreData</string>
                            </array>
                        </dict>
                    </array>
                </dict>
            </dict>
        </dict>
    </array>
    <key>Targets</key>
    <array>
        <dict>
            <key>Dependencies</key>
            <array>
                <integer>0</integer>
            </array>
            <key>Frameworks</key>
            <array>
                <string>UIKit</string>
                <string>Foundation</string>
            </array>
            <key>SharedSettings</key>
            <dict>
                <key>BUNDLE_LOADER</key>
                <string>$(BUILT_PRODUCTS_DIR)/___PARENTPACKAGENAME___.app/___PARENTPACKAGENAME___</string>
                <key>FRAMEWORK_SEARCH_PATHS</key>
                <string>&quot;$(SDKROOT)/Developer/Library/Frameworks&quot; &quot;$(DEVELOPER_LIBRARY_DIR)/Frameworks&quot;</string>
                <key>GCC_PREFIX_HEADER</key>
                <string>___PARENTPACKAGENAME___/___PARENTPACKAGENAME___-Prefix.pch</string>
                <key>TEST_HOST</key>
                <string>$(BUNDLE_LOADER)</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
